{
  "name": "pdehaan.dev",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "version": "6.0.0",
  "author": "Peter deHaan <peter@deseloper.com>",
  "bugs": {
    "url": "https://github.com/pdehaan/pdehaan.dev/issues"
  },
  "dependencies": {
    "@11ty/eleventy": "^1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1"
  },
  "devDependencies": {},
  "homepage": "https://github.com/pdehaan/pdehaan.dev#readme",
  "keywords": [],
  "license": "MIT",
  "main": ".eleventy.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pdehaan/pdehaan.dev.git"
  },
  "scripts": {
    "bench": "DEBUG=Eleventy:Benchmark* npm run build",
    "build": "npx @11ty/eleventy",
    "debug": "DEBUG=* npm run build",
    "format": "npx prettier .eleventy.js config src www --write",
    "serve": "npm run build -- --serve",
    "start": "npm run build",
    "watch": "npm run build -- --watch"
  }
}
